<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp">

            <TextView
                android:id="@+id/heji2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="合计税额(单位/元)："
                android:textSize="18sp" />

            <TextView
                android:id="@+id/mTaxTime2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:visibility="gone"
                android:layout_marginRight="20dp"
                android:text="2017-11-20"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/mAllTotalTax2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/heji2"
                android:layout_marginRight="20dp"
                android:gravity="center_horizontal"
                android:text="00000"
                android:textColor="@android:color/holo_orange_light"
                android:textSize="35sp" />

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.001dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@android:color/darker_gray" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp">

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="企业所得税："
                android:textSize="16sp" />

            <TextView
                android:id="@+id/mQYSDtax2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:gravity="center"
                android:text=""
                android:textSize="16sp" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="应纳增值税："
                android:textSize="16sp" />

            <TextView
                android:id="@+id/mAddTax2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:gravity="center"
                android:text="000000"
                android:textSize="16sp" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.001dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@android:color/darker_gray" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="自开票收入：" />

                <TextView
                    android:id="@+id/mZKPtax2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="00000" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="无票收入：" />

                <TextView
                    android:id="@+id/mWPSRtax2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="00000" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="上期留抵：" />

                <TextView
                    android:id="@+id/mLastStay2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="000000" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="本期销项税额度：" />

                <TextView
                    android:id="@+id/mCurrentOutTax2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="000000" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="本期进项税额度：" />

                <TextView
                    android:id="@+id/mCurrentInTax2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="000000" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp">

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="合计收入：" />

                <TextView
                    android:id="@+id/mTotaltax2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="00000" />
            </LinearLayout>


        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.001dp"
            android:layout_margin="10dp"
            android:background="@android:color/darker_gray" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:gravity="center_vertical"
                android:text="附加税："
                android:textSize="16sp" />

            <TextView
                android:id="@+id/mAdditionTax2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="20dp"
                android:gravity="center_vertical"
                android:text="00000"
                android:textSize="16sp" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.001dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@android:color/darker_gray" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="城市维护建设税：" />

                <TextView
                    android:id="@+id/mCityMaintainTax2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="000000" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="地方教育附加税：" />

                <TextView
                    android:id="@+id/mLocoalTeachTax2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="000000" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="城市教育附加税：" />

                <TextView
                    android:id="@+id/mCityTeachTax2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="000000" />
            </LinearLayout>


        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.001dp"
            android:layout_margin="5dp"
            android:background="@android:color/darker_gray" />

        <LinearLayout
            android:id="@+id/mSalaryBiao2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:orientation="vertical"
            android:visibility="gone">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="工资表"
                android:textSize="18sp" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/mSocialBiao2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:orientation="vertical"
            android:visibility="gone">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="社保表"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"
            >
            <View
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"/>

            <TextView
                android:id="@+id/mTax2"
                android:layout_width="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_height="wrap_content"
                android:text="报税人："
                android:textSize="16sp" />

            <TextView
                android:id="@+id/mTaxName2"
                android:layout_toRightOf="@id/mTax"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="蔡程安"
                android:layout_marginRight="20dp"
                android:textSize="16sp" />
        </LinearLayout>

    </LinearLayout>
    <!--小规模-->
</LinearLayout>